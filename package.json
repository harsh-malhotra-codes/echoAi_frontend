{
  "name": "echoai-chatbot",
  "version": "1.0.0",
  "description": "A space-themed AI chatbot built with React and Node.js",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "chatbot",
    "ai",
    "react",
    "vercel",
    "gemini"
  ],
  "author": "Harsh Malhotra",
  "license": "MIT"
}
